            <Popup Name="Tank_Info" IsOpen="{Binding OpenTankInfo,UpdateSourceTrigger=PropertyChanged}" Placement="Center" >
                <Grid HorizontalAlignment="Center"   Background="#00786B">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="25*" />
                        <RowDefinition Height="25*" />
                        <RowDefinition Height="25*" />
                        <RowDefinition Height="25*" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="50*" />
                        <ColumnDefinition Width="50*" />
                    </Grid.ColumnDefinitions>

                    <Button  Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Background="Red" Content="X" HorizontalAlignment="Right" VerticalAlignment="Top" Command="{Binding ExitTankInfoPopupCommand}"/>

                    <Label Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2"  Foreground="White" HorizontalAlignment="Center" Content="Tank Real Time Values" Margin="10" FontFamily="Bold" FontSize="25"/>

                    <Label Grid.Row="2" Grid.Column="0" Foreground="White"  Content="{Binding PopupStationNo}" />
                    <Label Grid.Row="2" Grid.Column="1" Foreground="White" Content="{Binding PopupStationName}"/>

                    <!--<GroupBox Grid.Row="3" Grid.Column="0" Margin="10" Background="LightBlue">-->
                    <Grid HorizontalAlignment="Center" Grid.Row="3" Grid.Column="0" Margin="10" Background="LightBlue">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>


                            <Label Grid.Row="0" Grid.Column="0"   Foreground="Black"  Content="Load No" />
                            <Label Grid.Row="1" Grid.Column="0"   Foreground="Black"  Content="Part Name" />
                            <Label Grid.Row="2" Grid.Column="0"   Foreground="Black"  Content="Set Current" />
                            <Label Grid.Row="3" Grid.Column="0"   Foreground="Black"  Content="Actual Current" />

                            <Label Grid.Row="0" Grid.Column="1"  Foreground="Black"   Content="{Binding PopupLoadNumber}"/>
                            <Label Grid.Row="1" Grid.Column="1"   Foreground="Black"   Content="{Binding PopupPartName}" />
                            <Label Grid.Row="2" Grid.Column="1"   Foreground="Black"  Content="{Binding PopupSetCurrent}"  />
                            <Label Grid.Row="3" Grid.Column="1"   Foreground="Black"  Content="{Binding PopupActualCurrent}"  />
                        </Grid>
                    <!--</GroupBox>-->

                    <!--<GroupBox Grid.Row="3" Grid.Column="1" Margin="10" Background="LightBlue">-->
                        <Grid HorizontalAlignment="Center" Grid.Row="3" Grid.Column="1" Margin="10" Background="LightBlue">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>

                            <Label Grid.Row="1" Grid.Column="0"    Foreground="Black" Content="Acutal" />
                            <Label Grid.Row="2" Grid.Column="0"   Foreground="Black"  Content="Low" />
                            <Label Grid.Row="3" Grid.Column="0"   Foreground="Black"  Content="High" />

                            <Label Grid.Row="0" Grid.Column="1"   Foreground="Black"  Content="Temperature" />
                            <Label Grid.Row="1" Grid.Column="1"   Foreground="Black"  Content="{Binding PopupActualTemp}" />
                            <Label Grid.Row="2" Grid.Column="1"   Foreground="Black"  Content="{Binding PopupLowTemp}"/>
                            <Label Grid.Row="3" Grid.Column="1"   Foreground="Black"  Content="{Binding PopupHighTemp}" />

                            <Label Grid.Row="0" Grid.Column="2"  Foreground="Black"   Content="pH" />
                            <Label Grid.Row="1" Grid.Column="2"   Foreground="Black"  Content="{Binding PopupActualpH}" />
                            <Label Grid.Row="2" Grid.Column="2"   Foreground="Black"  Content="{Binding PopupLowpH}" />
                            <Label Grid.Row="3" Grid.Column="2"   Foreground="Black" Content="{Binding PopupHighpH}" />
                        </Grid>
                    <!--</GroupBox>-->

                </Grid>
            </Popup>


        </Grid>

     
